    "def divs_and_max(num):\n",
    "    max_div = 0\n",
    "    for i in range(1, num+1):\n",
    "        if num % i == 0:\n",
    "            print(i)\n",
    "            max_div = i\n",
    "    return max_div\n",
    "\n",
    "number = int(input())\n",
    "largest_divisible = divs_and_max(number)\n",
    "print(\"Max divisible integer is:\", largest_divisible)\n"
   ]
